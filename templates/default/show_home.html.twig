{% extends 'base.html.twig' %}

{% block title %}Accueil
{% endblock %}

{% block body %}

	<div class="container">
		<div id="carouselExampleIndicators" class="carousel slide " data-bs-ride="carousel">
			<div class="carousel-indicators">
				<button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
				<button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="1" aria-label="Slide 2"></button>
				<button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="2" aria-label="Slide 3"></button>
			</div>
			<div class="carousel-inner  ">
				<div class="carousel-item active  ">
					<img class="w-100" src="{{ asset('uploads/' ~ sliders1.photo) }}" alt="First slide">
				</div>
				<div class="carousel-item r ">
					<img class="w-100" src="{{ asset('uploads/' ~ sliders2.photo) }}" alt="First slide">
				</div>
				<div class="carousel-item   ">
					<img class="w-100" src=" {{ asset('uploads/' ~ sliders3.photo) }} " alt=" first slide">
				</div>
			</div>
			<button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide="prev">
				<span class="carousel-control-prev-icon" aria-hidden="true"></span>
				<span class="visually-hidden">Previous</span>
			</button>
			<button class="carousel-control-next" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide="next">
				<span class="carousel-control-next-icon" aria-hidden="true"></span>
				<span class="visually-hidden">Next</span>
			</button>
		</div>
	</div>
	<br>
	<br>
	<br>
	<div class='row d-flex justify-content-around'>

		<div class="card text-center " style="width: 18rem;">
			<img class="border rounded-circle" src="https://static-otelico.com/cache/regina_berck/hotel_regina-berck_sur_mer-chambre_premium_1.jpg" class="card-img-top" alt="...">
			<div class="card-body">
				<h5 class="card-title">Chambre</h5>
				<p class="card-text">Venez découvrir nos chambres pour votre plus grand confort !</p>
				<a href="#" class="btn btn-primary">Découvrir les chambres</a>
			</div>
		</div>
		<div class="card text-center" style="width: 18rem;">
			<img class="border rounded-circle" src="https://cdn.groupedelion.com/rb/img/restaurant-bar/restaurant-rois-gastronomique-etoile-l.jpg" class="card-img-top" alt="...">
			<div class="card-body">
				<h5 class="card-title">Restaurant</h5>
				<p class="card-text">Nos plats sont confectionnés à la minute par notre chef cuisinier chaque jour.</p>
				<a href="{{ path('show_restaurant') }}" class="btn btn-primary">Voir la carte</a>
			</div>
		</div>
		<div class="card text-center" style="width: 18rem;">
			<img class="border rounded-circle" src="data:image/jpeg;base64," class="card-img-top" alt="...">
			<div class="card-body">
				<h5 class="card-title">Spa</h5>
				<p class="card-text">Détendez-vous et offrez vous un massage dont vous vous rappellerez longtemps.</p>
				<a href="{{ path('show_spa') }}" class="btn btn-primary">Les soins</a>
			</div>
		</div>

	</div>

{% endblock %}
