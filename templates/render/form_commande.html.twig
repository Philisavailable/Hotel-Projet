{{ form_start(form, {action: path('create_commande', {'id': app.request.attributes.get('id')})}) }}
<div class="col-6 mx-auto mt-3">
	<div class="row">
		<div class="col-6">{{ form_row(form.dateDebut) }}</div>
		<div class="col-6">{{ form_row(form.dateFin) }}</div>
	</div>

	<div class="row">
		<div class="col-6">{{ form_row(form.prenom) }}</div>
		<div class="col-6">{{ form_row(form.nom) }}</div>
	</div>
	<div class="row">
		<div class="col-6">{{ form_row(form.telephone) }}</div>
		<div class="col-6">{{ form_row(form.email) }}</div>
	</div>
	<div class="row mx-auto">
		{{ form_row(form.submit) }}
	</div>
</div>
{{ form_end(form) }}
